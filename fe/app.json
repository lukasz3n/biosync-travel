{
  "expo": {
    "name": "BioSync Travel",
    "slug": "biosync-travel",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/react-logo.png",
    "scheme": "biosynctravel",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.biosync.travel",
      "config": {
        "googleMapsApiKey": "AIzaSyANSVRoQv7Dqhgis1boaYlKiKx3EfXNFnY"
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/react-logo.png",
        "backgroundImage": "./assets/images/react-logo.png",
        "monochromeImage": "./assets/images/react-logo.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.biosync.travel",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyANSVRoQv7Dqhgis1boaYlKiKx3EfXNFnY"
        }
      }
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/react-logo.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}
